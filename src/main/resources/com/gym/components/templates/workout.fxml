<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="320.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gym.controller.templates.WorkoutController">
   <children>
      <HBox prefHeight="30.0" prefWidth="600.0" style="-fx-background-color: FFDD55;">
         <children>
            <Button mnemonicParsing="false" onAction="#coaches" prefHeight="30.0" prefWidth="225.0" styleClass="navBtn" stylesheets="@../../styles/btn.css" text="ТРЕНЕРЫ">
               <font>
                  <Font name="Arial Black" size="12.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button mnemonicParsing="false" onAction="#myWorkouts" prefHeight="30.0" prefWidth="225.0" styleClass="navBtn" stylesheets="@../../styles/btn.css" text="МОИ ТРЕНИРОВКИ">
               <font>
                  <Font name="Arial Black" size="12.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </children>
      </HBox>
      <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="290.0" prefWidth="450.0" style="-fx-background-color: FFFAAA;">
        <content>
            <VBox fx:id="wrap_workouts" prefHeight="288.0" prefWidth="448.0" style="-fx-background-color: FFFAAA;" />
        </content>
      </ScrollPane>
   </children>
</VBox>
